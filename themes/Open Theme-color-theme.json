{
  "name": "Open Theme",
  "colors": {
    "editor.foreground": "#ffffff",
    "editor.background": "#212121",
    "sideBarTitle.foreground": "#98ff43",
    "activityBarBadge.background": "#dd6520"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#676767"
      }
    },
    {
      "scope": "punctuation.definition.comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#676767"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#71f1a6"
      }
    },
    {
      "scope": "string constant.other.placeholder",
      "settings": {
        "foreground": "#e3f15c"
      }
    },
    {
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#b1b1b1"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ffee00"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#c460dd"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#eea56a"
      }
    }
  ]
}
